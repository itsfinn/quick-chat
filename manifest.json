{
    "manifest_version": 3,
    "name": "Quick Chat",
    "version": "0.0.1",
    "description":"Quick Chat To Deepseek, Doubao, yuanbao",
    "icons": {
        "16": "icons/at.png",
        "48": "icons/at.png",
        "128": "icons/at.png"
    },
    "permissions": [
        "omnibox",
        "scripting",
        "tabs",
        "storage" 
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "omnibox": {
        "keyword": "@qc"
    },
    "options_ui": {
        "page": "options/options.html",
        "open_in_tab": true
    }
}
