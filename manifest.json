{
    "manifest_version": 3,
    "name": "Quick Chat To",
    "version": "1.0",
    "description":"Quick Chat To Deepseek, Doubao, yuanbao",
    "icons": {
        "16": "icons/at.png",
        "48": "icons/at.png",
        "128": "icons/at.png"
    },
    "permissions": [
        "omnibox",
        "scripting",
        "tabs",
        "storage" 
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "omnibox": {
        "keyword": "@qct"
    },
    "options_page": "options.html" 
}
